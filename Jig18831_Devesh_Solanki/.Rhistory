library('readxl')
data=read_excel('data.xlsx',sheet=1,na='-')
data$UNMET_NEED
data_prep<-data%>%mutate(rank=ntile(UNMET_NEED,10))%>%count(RETURNED_2ND_YR,rank)%>%filter(RETURNED_2ND_YR==1)
data_prep$N<-unclass(data%>%mutate(rank=ntile(UNMET_NEED,10))%>%count(rank)%>%unname())[[2]]
data_prep$percent<-data_prep$n/data_prep$N
data_prep$greater_than<-unclass(data%>%mutate(rank=ntile(UNMET_NEED,10))%>%group_by(rank)%>%summarise(min(UNMET_NEED))%>%unname())[[2]]
data_prep$less_than<-unclass(data%>%mutate(rank=ntile(UNMET_NEED,10))%>%group_by(rank)%>%summarise(max(UNMET_NEED))%>%unname())[[2]]
data_prep$var_name<-rep("UNMET_NEED",nrow(data_prep))
write.csv(data_prep,"UNMET_NEED.csv",row.names=FALSE)
